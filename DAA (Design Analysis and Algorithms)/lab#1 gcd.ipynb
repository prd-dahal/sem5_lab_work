   "source": [
    "#lab 1 WAP to find the GCD\n",
    "no1 = int(input(\"Enter the first number::\"))\n",
    "no2 = int(input(\"Enter the second number::\"))\n",
    "gcd = 0\n",
    "for i in range(1,min(no1,no2)):\n",
    "    if(no1%i == 0 and no2%i == 0):\n",
    "         gcd = i\n",
    "    else:\n",
    "        pass\n",
    "print('GCD of {} and {} is {}'.format(no1,no2,gcd))"
   ]
